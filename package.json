{
  "name": "ember-cli-less",
  "version": "2.0.1",
  "description": "Use less to preprocess your ember-cli app css",
  "keywords": [
    "ember-addon",
    "less",
    "css",
    "preprocessor"
  ],
  "repository": "https://github.com/gdub22/ember-cli-less",
  "license": "MIT",
  "author": "Garth Poitras <garth@bustle.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "release": "standard-version"
  },
  "dependencies": {
    "broccoli-less-single": "^1.0.1",
    "broccoli-merge-trees": "^1.0.0",
    "ember-cli-version-checker": "^1.1.4",
    "lodash.merge": "^3.3.2"
  },
  "devDependencies": {
    "@ember/optional-features": "^0.6.3",
    "ember-ajax": "^3.1.0",
    "ember-cli": "~3.5.1",
    "ember-cli-babel": "^6.16.0",
    "ember-cli-dependency-checker": "^3.0.0",
    "ember-cli-eslint": "^4.2.3",
    "ember-cli-htmlbars": "^3.0.0",
    "ember-cli-template-lint": "^1.0.0-beta.1",
    "ember-load-initializers": "^1.1.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^3.4.1",
    "ember-resolver": "^5.0.1",
    "ember-source": "~3.5.1",
    "ember-source-channel-url": "^1.1.0",
    "ember-try": "^1.0.0",
    "eslint-plugin-ember": "^5.2.0",
    "eslint-plugin-node": "^7.0.1",
    "loader.js": "^4.7.0",
    "qunit-dom": "^0.8.0",
    "standard-version": "^4.4.0"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "bugs": {
    "url": "https://github.com/gdub22/ember-cli-less/issues"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
